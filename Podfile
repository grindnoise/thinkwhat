source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0' # or platform :osx, '10.10' if your target is OS X.
use_frameworks!

target 'ThinkWhat' do
    pod 'Swinject'
    pod 'Alamofire'
    pod 'SwiftyJSON'
    pod 'FBSDKLoginKit'
    pod 'VK-ios-sdk'
    pod "YoutubePlayer-in-WKWebView", "~> 0.3.0"
    target 'ThinkWhatTests' do
	pod 'Quick'
	pod 'Nimble'
    end
end
