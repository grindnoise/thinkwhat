source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '13.0' # or platform :osx, '10.10' if your target is OS X.

target 'ThinkWhat' do
    use_frameworks!
    pod 'GoogleSignIn'
    pod 'ReCaptcha'
    pod 'SwiftyVK'
    pod "TTRangeSlider"
    pod 'SwiftyJSON'
    pod 'FBSDKCoreKit'
    pod 'FBSDKLoginKit'
    pod 'L10n-swift', '~> 5.10'
    pod "Agrume"
    pod "FlagKit"
    pod 'FBSDKShareKit'
    pod 'Swinject', '2.7.1'
    pod 'Alamofire', '~> 5.5'
    pod 'UIView-Shimmer', '~> 1.0'
    pod "YoutubePlayer-in-WKWebView", "~> 0.3.0"
    target 'ThinkWhatTests' do
	pod 'Quick'
	pod 'Nimble'
    end
end
