    use_frameworks!
    pod 'SwiftyVK'
    pod "TTRangeSlider"
    pod 'SwiftyJSON'
    pod 'FBSDKCoreKit'
    pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
